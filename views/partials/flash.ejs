<% for (const [category, messageList] of Object.entries(messages)) { %>
<% for (let message of messageList) { %>
<div class="alert alert-<%= category === 'error' ? 'danger' : category %> alert-dismissible fade show" role="alert">
  <%= message %>
  <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>
<% } %>